{
  "module": {
    "icu": {
      "source": "https://github.com/unicode-org/icu/releases/download/release-{ICU_VERSION}/icu4c-{ICU_VERSION_}-src.tgz",
      "version": "{ICU_VERSION}"
    },
    "libexpat": {
